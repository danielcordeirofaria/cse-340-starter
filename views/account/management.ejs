<h1><%= title %></h1>
<% if (messages && messages.notice) { %>
<p class="notice"><%= messages.notice %></p>
<% } %> <% if (errors) { %>
<ul class="errors">
  <% errors.forEach(error => { %>
  <li><%= error.msg %></li>
  <% }) %>
</ul>
<% } %> <% if (locals.loggedin) { %>
<p>Welcome, <%= locals.accountData.account_firstname %>! You're logged in.</p>
<% } else { %>
<p>You're logged in</p>
<% } %>